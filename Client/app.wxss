/**app.wxss**/
page{
  background-color: #eeeeee
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.div{
  margin: auto;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  margin-top: 2%;
  background-color: #FFFFFF;
}

@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1040812_k4u1x0gn1ef.eot?t=1550297747522'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1040812_k4u1x0gn1ef.eot?t=1550297747522#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1040812_k4u1x0gn1ef.woff?t=1550297747522') format('woff'),
  url('//at.alicdn.com/t/font_1040812_k4u1x0gn1ef.ttf?t=1550297747522') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1040812_k4u1x0gn1ef.svg?t=1550297747522#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 24px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-plus:before {
  content: "\e616";
}

.icon-minus:before {
  content: "\e617";
}

.icon-clean:before {
  content: "\e682";
}



